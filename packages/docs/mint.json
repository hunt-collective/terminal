{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Terminal",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#FF5E00",
    "light": "#FF5E00",
    "dark": "#FF5E00",
    "background": {
      "light": "#FFFFFF",
      "dark": "#000000"
    }
  },
  "theme": "quill",
  "layout": "topnav",
  "tabs": [
    {
      "name": "Writing Content",
      "url": "content"
    },
    {
      "name": "API References",
      "url": "api-playground"
    }
  ],
  "font": {
    "headings": {
      "family": "Geist Mono"
    },
    "body": {
      "family": "Geist Mono"
    }
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "rounded": "sharp",
  "codeBlock": {
    "mode": "auto"
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:hi@mintlify.com"
    }
  ],
  "topAnchor": {
    "name": "Home"
  },
  "anchors": [
    {
      "name": "Documentation",
      "icon": "book-open-cover",
      "url": "https://mintlify.com/docs"
    },
    {
      "name": "Community",
      "icon": "slack",
      "url": "https://mintlify.com/community"
    },
    {
      "name": "Blog",
      "icon": "newspaper",
      "url": "https://mintlify.com/blog"
    },
    {
      "name": "API Reference",
      "openapi": "https://app.stainlessapi.com/api/spec/documented/terminal",
      "url": "docs",
      "icon": "square-terminal"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": ["introduction", "quickstart", "development", "product"]
    },
    {
      "group": "Essentials",
      "pages": [
        "essentials/markdown",
        "essentials/code",
        "essentials/images",
        "essentials/settings",
        "essentials/navigation",
        "essentials/reusable-snippets"
      ]
    },
    {
      "group": "API Documentation",
      "pages": ["api-reference/introduction"]
    },
    {
      "group": "Endpoint Examples",
      "pages": [
        "api-reference/endpoint/get",
        "api-reference/endpoint/create",
        "api-reference/endpoint/delete"
      ]
    }
  ],
  "footerSocials": {
    "x": "https://x.com/terminaldotshop",
    "github": "https://github.com/terminaldotshop"
  }
}
