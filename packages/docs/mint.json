{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Terminal",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#FF5E00",
    "light": "#FF5E00",
    "dark": "#FF5E00",
    "background": {
      "light": "#FFFFFF",
      "dark": "#000000"
    }
  },
  "theme": "quill",
  "layout": "topnav",
  "tabs": [
    {
      "name": "Home",
      "url": "en/home"
    },
    {
      "name": "Products",
      "url": "en/products"
    },
    {
      "name": "Guides",
      "url": "en/guides"
    },
    {
      "name": "API Reference",
      "url": "en/api",
      "openapi": "https://app.stainlessapi.com/api/spec/documented/terminal"
    }
  ],
  "primaryTab": {
    "name": "Home"
  },
  "font": {
    "headings": {
      "family": "Geist Mono"
    },
    "body": {
      "family": "Geist Mono"
    }
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "rounded": "sharp",
  "codeBlock": {
    "mode": "auto"
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:support@terminal.shop"
    }
  ],
  "navigation": [
    {
      "group": "Home",
      "pages": ["en/home"]
    },
    {
      "group": "Products",
      "pages": ["en/products/index"]
    },
    {
      "group": "Guides",
      "pages": [
        "en/guides/markdown",
        "en/guides/code",
        "en/guides/images",
        "en/guides/settings",
        "en/guides/navigation",
        "en/guides/reusable-snippets"
      ]
    },
    {
      "group": "Using the API",
      "pages": ["en/api/getting-started", "en/api/errors", "en/api/client-sdks"]
    }
  ],
  "footerSocials": {
    "x": "https://x.com/terminaldotshop",
    "github": "https://github.com/terminaldotshop"
  }
}
