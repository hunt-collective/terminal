{
  "name": "@textjs/core",
  "version": "0.0.1",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "bun run script/build.ts"
  },
  "devDependencies": {
    "@types/react": "19.0.8",
    "@types/react-reconciler": "0.28.9",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "classnames": "2.5.1",
    "glob": "11.0.1",
    "react-reconciler": "0.31.0",
    "yoga-layout": "3.2.1"
  },
  "peerDependencies": {
    "@types/react": ">=19.0.0",
    "react": ">=19.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    },
    "./router": {
      "import": "./dist/router/index.js",
      "types": "./dist/router/index.d.ts"
    },
    "./router/utilities": {
      "import": "./dist/router/utilities.js",
      "types": "./dist/router/utilities.d.ts"
    },
    "./router/*": "./dist/router/*",
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./components/*": "./dist/components/*",
    "./config": {
      "import": "./dist/config/index.js",
      "types": "./dist/config/index.d.ts"
    },
    "./config/*": "./dist/config/*",
    "./vite": {
      "import": "./dist/vite-plugin-textjs.js",
      "types": "./dist/vite-plugin-textjs.d.ts"
    },
    "./tsconfigs/*.json": "./tsconfigs/*",
    "./tsconfigs/*": "./tsconfigs/*.json"
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "vite": [
        "./dist/vite-plugin-textjs.d.ts"
      ],
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "prettier": {
    "semi": false
  }
}
