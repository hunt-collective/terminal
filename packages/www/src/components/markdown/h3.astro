---
import type { HTMLAttributes } from 'astro/types'
import Line from '@components/line.astro'

interface Props extends HTMLAttributes<'h3'> {}
const { class: _, ...props } = Astro.props
---

<Line />
<Line class="bg-gray-1/60">
  <h3 {...props} class="uppercase text-gray-11 scroll-m-20 lg:scroll-m-10">
    <slot />
  </h3>
</Line>
