---
import type { HTMLAttributes } from 'astro/types'
import Line from '@components/line.astro'

interface Props extends HTMLAttributes<'h5'> {}
const { class: _, ...props } = Astro.props
---

<Line class="bg-gray-1">
  <h5 {...props} class="text-gray-11 scroll-m-20 lg:scroll-m-10">
    <slot />
  </h5>
</Line>
