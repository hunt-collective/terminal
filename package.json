{
  "name": "terminal-shop",
  "version": "0.0.0",
  "sideEffects": false,
  "scripts": {
    "dev": "sst dev",
    "typecheck": "bun --filter=* typecheck",
    "sso": "aws sso login --sso-session=terminal --no-browser",
    "sdk": "sst shell ./packages/scripts/stainless",
    "openapi": "sst shell ./packages/scripts/openapi",
    "cli": "cd ./packages/go && sst shell --stage=dev go run ./cmd/cli/main.go"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@openauthjs/openauth": "0.3.1",
    "hono": "4.6.12",
    "linkedom": "0.18.6",
    "patch-package": "^8.0.0",
    "remeda": "2.3.0",
    "sst": "3.6.3"
  },
  "overrides": {
    "zod": "3.24.1"
  },
  "devDependencies": {
    "typescript": "5.5.4"
  },
  "patchedDependencies": {
    "hono-openapi@0.2.1": "patches/hono-openapi@0.2.1.patch"
  }
}
